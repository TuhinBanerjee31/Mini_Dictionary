{
    "name": "Mini Dictionary",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "book.png",
        "default_title": "Lookup a definition!"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["script.js"]
        }
      ],
    "icons":{
        "128": "book.png"
    }
}